{
    "name": "luminary-api",
    "private": true,
    "scripts": {
        "build": "nest build",
        "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
        "start": "nest start",
        "start:dev": "nest start --watch",
        "dev": "npm run start:dev",
        "start:debug": "nest start --debug --watch",
        "start:prod": "node dist/main",
        "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
        "lint:fix": "npm run lint -- --fix",
        "test:unit": "npm run test:watch",
        "test": "dotenv -e .env.test -- jest --forceExit",
        "test:watch": "dotenv -e .env.test -- jest --watch --forceExit",
        "test:detect": "dotenv -e .env.test -- jest --detectOpenHandles",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e": "jest --config ./test/jest-e2e.json",
        "seed": "nest start -- seed"
    },
    "dependencies": {
        "@nestjs/common": "^10.0.0",
        "@nestjs/config": "^3.1.1",
        "@nestjs/core": "^10.0.0",
        "@nestjs/jwt": "^10.2.0",
        "@nestjs/platform-express": "^10.3.7",
        "@nestjs/platform-socket.io": "^10.3.1",
        "@nestjs/websockets": "^10.3.1",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.14.1",
        "jsonwebtoken": "^9.0.2",
        "nano": "^10.1.3",
        "reflect-metadata": "^0.1.13",
        "rxjs": "^7.8.1",
        "transliteration": "^2.3.5"
    },
    "devDependencies": {
        "@nestjs/cli": "^10.0.0",
        "@nestjs/schematics": "^10.0.0",
        "@nestjs/testing": "^10.0.0",
        "@types/express": "^4.17.17",
        "@types/jest": "^29.5.2",
        "@types/node": "^20.3.1",
        "@types/supertest": "^6.0.0",
        "@typescript-eslint/eslint-plugin": "^6.0.0",
        "@typescript-eslint/parser": "^6.0.0",
        "dotenv-cli": "^7.3.0",
        "eslint": "^8.42.0",
        "jest": "^29.5.0",
        "prettier": "^3.0.0",
        "socket.io-client": "^4.7.4",
        "source-map-support": "^0.5.21",
        "supertest": "^6.3.3",
        "ts-jest": "^29.1.0",
        "ts-loader": "^9.4.3",
        "ts-node": "^10.9.1",
        "tsconfig-paths": "^4.2.0",
        "typescript": "^5.1.3"
    }
}
