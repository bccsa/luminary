        "@vue/eslint-config-typescript": "^12.0.0",
        "@vue/test-utils": "^2.4.3",
        "@vue/tsconfig": "^0.5.0",
        "autoprefixer": "^10.4.16",
        "eslint": "^8.49.0",
        "eslint-plugin-vue": "^9.17.0",
        "fake-indexeddb": "^5.0.2",
        "jsdom": "^23.0.1",
        "npm-run-all2": "^6.1.1",
        "postcss": "^8.4.33",
        "prettier": "^3.0.3",
        "prettier-plugin-tailwindcss": "^0.5.11",
        "tailwindcss": "^3.4.1",
        "typescript": "~5.3.0",
